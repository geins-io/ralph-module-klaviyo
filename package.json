{
  "name": "@geins/ralph-module-klaviyo",
  "version": "1.0.0",
  "description": "Klaviyo module for Geins PWA Storefront Ralph",
  "author": "arvidsson-geins",
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "publish": "npm publish --access public",
    "link": "sudo npm link",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write"
  },
  "devDependencies": {
    "jest": "^23.4.1",
    "nuxt": "^2.4.0",
    "prettier": "^1.16.1",
    "puppeteer": "^1.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {}
}
